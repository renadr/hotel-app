{
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-filename-extension": 0,
      "react/prop-types": 0,
      "react/no-array-index-key": 0,
      "global-require": 0,
      "no-await-in-loop": 0,
      "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
      "class-methods-use-this": 0,
      "react/no-multi-comp": 0
    }
  }
  